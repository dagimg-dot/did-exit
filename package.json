{
	"name": "did-exit",
	"version": "1.6.0",
	"description": "Exam Generator from PDF - Upload your exam PDF and make it interactive",
	"type": "module",
	"scripts": {
		"dev": "live-server --port=8000",
		"dev:watch": "live-server --port=8000 --watch=.",
		"dev:netlify": "netlify dev --live",
		"format": "biome format . --write",
		"format:check": "biome format .",
		"lint": "biome lint .",
		"lint:fix": "biome lint . --write",
		"check": "biome check .",
		"check:fix": "biome check . --write"
	},
	"keywords": [
		"pdf",
		"exam",
		"quiz",
		"ai",
		"education",
		"javascript"
	],
	"author": "Dagim G. Astatkie",
	"license": "MIT",
	"devDependencies": {
		"@biomejs/biome": "^2.0.0",
		"live-server": "^1.2.2",
		"netlify-cli": "^22.1.3"
	},
	"engines": {
		"node": ">=18.0.0"
	},
	"dependencies": {
		"slugify": "^1.6.6"
	}
}
